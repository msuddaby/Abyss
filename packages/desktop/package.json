{
  "name": "abyss-desktop",
  "productName": "Abyss",
  "version": "1.4.33",
  "description": "Abyss Desktop - Electron wrapper for Abyss web client",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "tsc && NODE_ENV=development electron .",
    "start": "electron .",
    "build": "tsc",
    "rebuild": "electron-rebuild",
    "postinstall": "electron-rebuild",
    "make": "electron-forge make",
    "make:win": "electron-builder --win --publish never",
    "make:full": "cd ../../client && ELECTRON=true npx vite build && cd ../packages/desktop && tsc && electron-forge make",
    "package": "electron-forge package"
  },
  "keywords": [],
  "author": "Abyss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/msuddaby/Abyss.git"
  },
  "dependencies": {
    "auto-launch": "^5.0.6",
    "electron-log": "^5.1.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "uiohook-napi": "^1.5.4"
  },
  "optionalDependencies": {
    "electron-squirrel-startup": "^1.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-dmg": "^7.5.0",
    "electron-builder": "^25.1.8",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.7.2",
    "@reforged/maker-appimage": "^5.2.0",
    "@types/node": "^22.0.0",
    "electron": "^33.0.0",
    "typescript": "^5.0.0"
  }
}
